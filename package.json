{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "snippetslab",
  "title": "SnippetsLab",
  "description": "Search your SnippetsLab snipets from Raycast",
  "icon": "command-icon.png",
  "author": "huangsemao",
  "categories": [
    "Developer Tools", "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Find in SnippetsLab",
      "description": "Search snippets for the provided string",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.41.1",
    "@raycast/utils": "^1.4.8",
    "execa": "6.1.0",
    "html-entities": "2.3.2",
    "fast-xml-parser": "3.20.3",
    "tempy": "2.0.0",
    "raycast-toolkit": "1.0.3"
  },
  "devDependencies": {
    "@types/node": "16.10.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}